<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension-point id="zilliansFlexPoint" name="%extension-point.name.1" schema="schema/zilliansFlexPoint.exsd"/>

   <extension
         id="*"
         name="%extension.name"
         point="org.eclipse.ui.actionSets">
      <actionSet
            label="%actionSet.label.1"
            visible="true"
            id="com.zillians.run-zillians-run.zs-flex-project.actionSet">
         <menu
               label="%menu.label"
               id="sampleMenu">
            <separator
                  name="sampleGroup">
            </separator>
         </menu>
         <action
               class="com.zillians.run_zillians_run.zs_flex_project.actions.ZccFlexAction"
               icon="icons/zcc.png"
               id="com.zillians.runzilliansrun.zsflexproject.actions.ZccFlexAction"
               label="%action.label"
               menubarPath="Zillians/zilliansGroup"
               state="false"
               toolbarPath="zilliansGroup"
               tooltip="%action.tooltip">
         </action>
         <action
               class="com.zillians.run_zillians_run.zs_flex_project.actions.UploadAction"
               icon="icons/sample.gif"
               id="com.zillians.runzilliansrun.zsflexproject.actions.upload.UploadAction"
               label="&amp;Upload Action"
               menubarPath="sampleMenu/sampleGroup"
               state="true"
               style="push"
               toolbarPath="sampleGroup"
               tooltip="Hello, Eclipse world">
         </action>
      </actionSet>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="com.zillians.run_zillians_run.zs_flex_project.preferences.ZilliansPreferencePage"
            id="com.zillians.runzilliansrun.zsflexproject.preferences.ZilliansPreferencePage"
            name="%page.name.1">
      </page>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="com.zillians.run_zillians_run.zs_flex_project.preferences.PreferenceInitializer">
      </initializer>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="com.zillians.run_zillians_run.zs_flex_project.editors.zseditor.ZsEditor"
            contributorClass="org.eclipse.ui.texteditor.BasicTextEditorActionContributor"
            extensions="zs"
            icon="icons/sample.gif"
            id="com.zillians.runzilliansrun.zsflexproject.editors.ZsEditor"
            name="%editor.name.3">
      </editor>
      <editor
            class="com.zillians.run_zillians_run.zs_flex_project.editors.zxmleditor.ZxmlMultiPageEditor"
            contributorClass="com.zillians.run_zillians_run.zs_flex_project.editors.zxmleditor.ZxmlMultiPageEditorContributor"
            extensions="zxml"
            icon="icons/sample.gif"
            id="com.zillians.runzilliansrun.zsflexproject.editors.ZxmlMultiPageEditor"
            name="%editor.name.4">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.startup">
      <startup
            class="com.zillians.run_zillians_run.zs_flex_project.ZccStartup">
      </startup>
   </extension>

</plugin>
